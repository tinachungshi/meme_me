<h1>Log in!</h1>

<p><%= alert %></p>
<%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div>
    <%= submit_tag "Log In" %>
  </div><br>
  <div>
    <p>Become a MEME member <%= link_to "today" , '/register' %></p>
  </div>
<% end %>
